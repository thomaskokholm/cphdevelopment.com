{
  "name": "angular2-aot",
  "version": "0.0.0",
  "description": "Angular2 Ahead-of-Time (AoT) Compilation",
  "homepage": "https://github.com/r-park/angular2-aot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-park/angular2-aot.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "run-s build:ngc build:webpack",
    "build:ngc": "ngc",
    "build:webpack": "cross-env NODE_ENV=production webpack --display-chunks",
    "clean": "del-cli build dist",
    "prebuild": "npm run clean",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:webpack": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:webpack"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "core-js": "~2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.25"
  },
  "devDependencies": {
    "@types/node": "~6.0.41",
    "angular2-template-loader": "~0.5.0",
    "autoprefixer": "~6.5.0",
    "compression": "~1.6.2",
    "cross-env": "~3.0.0",
    "del-cli": "~0.2.0",
    "express": "~4.14.0",
    "html-webpack-plugin": "~2.22.0",
    "morgan": "~1.7.0",
    "nodemon": "~1.10.2",
    "npm-run-all": "~3.1.0",
    "postcss-loader": "~0.13.0",
    "raw-loader": "~0.5.1",
    "ts-helpers": "~1.1.1",
    "ts-loader": "~0.8.2",
    "typescript": "~2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~2.2.0"
  }
}
